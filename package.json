  "devDependencies": {
    "@openally/config.eslint": "^2.0.0",
    "@openally/config.typescript": "^1.2.1",
    "@slimio/is": "^2.0.0",
    "@types/node": "^25.0.3",
    "is-svg": "^6.0.0",
    "typescript": "^5.3.3"
